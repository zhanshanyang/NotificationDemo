<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yzs.providerdemo2">

    <uses-permission android:name="com.yzs.provider.Permission" />
    <uses-permission android:name="scut.carson_ho.Read"/>
    <uses-permission android:name="scut.carson_ho.Write"/>
    <uses-permission android:name="scut.carson_ho.PROVIDER"/>
    <uses-permission android:name="m01.surface.state.Read"/>
    <uses-permission android:name="m01.surface.state.Write"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>